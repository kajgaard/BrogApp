        <activity android:name="brog.coffee.brogapp.CreateNewBrew.EnterBrewTimeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="brog.coffee.brogapp.CreateNewBrew.EnterBloomTimeActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="brog.coffee.brogapp.CreateNewBrew.EnterBloomWaterActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="brog.coffee.brogapp.CreateNewBrew.EnterTempActivity"
